

<section class="complements-section">
  <h1>My Pants shown here</h1>
<div class="row">
  {{#each element}}
  <div class="content col-md-6 col-xl-4">
    <h2>{{pants_complement_name}}</h2>
    <img src="{{image}}" alt="Pants Complement Image" />
    <form action="one-pants-complement/{{_id}}" method="get">
      <button>See this look</button>
    </form>
    <form action="one-pants-complement/{{_id}}" method="post">
      <button>Delete Look</button>
    </form>
  </div>
  {{/each}}    
</div>

</section>
