

<section class="complements-section">
  <h1>My Shirt Complements shown here</h1>

  {{#each element}}
  <div>
    <h2>{{shirt_complement_name}}</h2>
    <img src="{{image}}" alt="Shirt Complement Image" />
    <form action="one-shirt-complement/{{_id}}" method="get">
      <button>See this look</button>
    </form>
    <form action="one-shirt-complement/{{_id}}" method="post">
      <button>Delete Look</button>
    </form>
  </div>
  {{/each}}  
</section>
