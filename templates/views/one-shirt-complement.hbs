

<section class="one-shirt-complement-container">
  {{#each element}}
  <div>
    <h1>{{shirt_complement_name}}</h1>
    <img src="{{image}}" alt="Shirt Complement Image" />
    <form action="{{_id}}" method="post">
      <button>Delete Complement</button>
    </form>
    <form action="/shirt-complements" method="get">
      <button>Go Back</button>
    </form>
  </div>
  {{/each}}  
</section>
